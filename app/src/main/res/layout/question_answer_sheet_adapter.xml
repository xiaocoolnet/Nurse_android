<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:gravity="center"
    >
    <LinearLayout
        android:id="@+id/line_answer_sheet"
        android:layout_width="@dimen/activity_size_50"
        android:layout_height="@dimen/activity_size_50"
        android:orientation="vertical"
        android:gravity="center"
        >
        <TextView
            android:id="@+id/answer_Num_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="1"
            android:textSize="@dimen/text_size_16"
            />
        <TextView
            android:id="@+id/answer_type"
            android:layout_width="15dp"
            android:layout_height="15dp"
            android:textSize="@dimen/text_size_16"
            android:background="@drawable/question_sheet_dot_silver"
            />
    </LinearLayout>
</LinearLayout>
